{
  "name": "@smartive/kubernetes-helpers",
  "version": "0.0.0-development",
  "description": "commandline tool that helps with kubernetes deployments",
  "main": "./build/kubernetes-helpers.js",
  "scripts": {
    "clean": "del-cli ./build ./out ./coverage",
    "build": "npm run clean && tsc -p ./config/tsconfig.build.json",
    "develop": "npm run clean && tsc -p .",
    "lint": "tslint -c ./tslint.json -p ./config/tsconfig.build.json",
    "test": "jest -c jest.json --watch",
    "test:ci": "jest -c jest.json --coverage --runInBand",
    "semantic-release": "semantic-release",
    "package": "npm run build && pkg --out-path out .",
    "link": "npm run build && npm link"
  },
  "bin": {
    "k8s": "./build/kubernetes-helpers.js"
  },
  "keywords": [
    "kubernetes",
    "k8s",
    "cli",
    "helper",
    "kubectl"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:smartive/open-source/kubernetes-helpers.git"
  },
  "bugs": {
    "url": "https://gitlab.com/smartive/open-source/kubernetes-helpers/issues"
  },
  "homepage": "https://gitlab.com/smartive/open-source/kubernetes-helpers",
  "author": "Christoph BÃ¼hler <christoph@smartive.ch>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/gitlab",
      "@semantic-release/npm"
    ],
    "prepare": [
      "@semantic-release/npm"
    ],
    "publish": [
      "@semantic-release/gitlab",
      "@semantic-release/npm"
    ],
    "success": false,
    "fail": false
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "clipboardy": "^1.2.3",
    "fast-glob": "^2.2.4",
    "find-up": "^3.0.0",
    "fs-extra": "^7.0.1",
    "got": "^9.3.2",
    "inquirer": "^6.2.0",
    "node-machine-id": "^1.1.10",
    "ora": "^3.0.0",
    "semver": "^5.6.0",
    "tslib": "^1.9.3",
    "yargonaut": "^1.1.4",
    "yargs": "^12.0.4"
  },
  "devDependencies": {
    "@semantic-release/gitlab": "^3.0.5",
    "@semantic-release/npm": "^5.1.1",
    "@smartive/tslint-config": "^4.0.0",
    "@types/clipboardy": "^1.1.0",
    "@types/fs-extra": "^5.0.4",
    "@types/got": "^9.2.0",
    "@types/inquirer": "0.0.43",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.6",
    "@types/semver": "^5.5.0",
    "@types/yargs": "^12.0.1",
    "del-cli": "^1.1.0",
    "jest": "^23.6.0",
    "memfs": "^2.14.0",
    "mock-fs": "^4.7.0",
    "pkg": "^4.3.4",
    "semantic-release": "^15.11.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tsutils": "^3.5.0",
    "typescript": "^3.1.6"
  }
}
